<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 
    <servlet>
        <servlet-name>addtracking</servlet-name>
        <servlet-class>com.shipment.tracking.app.AddShipmentGeoLocationController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addtracking</servlet-name>
        <url-pattern>/shipment/track/add</url-pattern>
    </servlet-mapping>
    
    
     <servlet>
        <servlet-name>gettracking</servlet-name>
        <servlet-class>com.shipment.tracking.app.GetShipmentGeoLocationController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>gettracking</servlet-name>
        <url-pattern>/shipment/track/get</url-pattern>
    </servlet-mapping>
    
    
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
 
